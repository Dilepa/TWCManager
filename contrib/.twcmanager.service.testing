[Unit]
Description=Tesla Wall Charger Manager

[Service]
Type=simple
User=twcmanager
WorkingDirectory=/tmp
ExecStart=/home/docker/.pyenv/shims/python3 -u -m TWCManager.TWCManager > /tmp/twcmanager-tests/twcmanager.service.output.log
Restart=always
StartLimitInterval=60s
StartLimitBurst=5

[Install]
WantedBy=multi-user.target
